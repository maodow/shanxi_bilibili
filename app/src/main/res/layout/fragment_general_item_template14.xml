<?xml version="1.0" encoding="utf-8"?>
<tv.huan.bilibili.widget.common.CommonRoundRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_200"
    android:focusable="true"
    app:rl_rate_width="0.7128">

    <include
        layout="@layout/common_poster"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <!-- 下标 -->
    <TextView
        android:id="@+id/album_item_index_template14"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/bg_shape_round"
        android:clickable="false"
        android:ellipsize="marquee"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:gravity="center"
        android:longClickable="false"
        android:singleLine="true"
        android:text="-"
        android:textColor="#ffffff"
        android:textSize="14sp" />

</tv.huan.bilibili.widget.common.CommonRoundRelativeLayout>